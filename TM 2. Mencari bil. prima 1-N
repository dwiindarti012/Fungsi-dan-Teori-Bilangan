#mencari bilangan prima 1-n
def cari_ptima(n):

    if n <= 1:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

#keluaran fungsi
angka = int(input("Masukkan batas bilangan: "))
print("Bilangan prima 1 sampai", angka, "adalah")
for i in range(1, angka + 1):
    if cari_prima(i):
        print(i, end=" ")
